a= "eu tenho um sonho."
b= " o sonho de ver meus filhos julgados por sua personalidade, não pela cor de sua pele."
c= "martin luther King júnior"

print(a.upper())
print (b.upper())
print (c.upper())

print (a.lower())
print (b.lower())
print (c.lower())

print (a.title())
print (b.title())
print (c.title())
